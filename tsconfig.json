{
  "include": [
    "src/*.ts"
  ],
  "compilerOptions": {
    "outDir": "./static",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noEmitOnError": false,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "lib": ["dom", "es6"],
    "target": "es6"
  }
}
